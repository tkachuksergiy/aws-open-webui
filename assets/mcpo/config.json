{
  "mcpServers": {
        "time": {
            "command": "uvx",
            "args": ["mcp-server-time", "--local-timezone=America/New_York"]
        },
        "gitlab": {
            "command": "npx",
            "args": ["-y", "@zereight/mcp-gitlab"],
            "env": {
                "GITLAB_API_URL": "https://gitlab.com/api/v4",
                "GITLAB_READ_ONLY_MODE": "false"
            }
        },
        "linear": {
            "command": "uvx",
            "args": [ "mcp-linear"]
        }
    }
}